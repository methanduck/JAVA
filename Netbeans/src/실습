    private void b2ActionPerformed(java.awt.event.ActionEvent evt) {                                   
 if(textField1.getText().equals("0"))
        {
        textField1.setText("2");
        }
        else
        {
            textField1.setText(textField1.getText()+"2");
         }
    }                                  

